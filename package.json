{
  "name": "start",
  "version": "0.1.0",
  "description": "",
  "author": "ostap",
  "private": true,
  "homepage": "./",
  "scripts": {
    "prerender": "rm -rf prerendered/* && node_modules/webpack/bin/webpack.js --hide-modules --mode none --config=webpack/prerender/webpack.config.js",
    "start": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --hot-styles",
    "start:legacy": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --hot-styles --env.legacy=true",
    "start:prod": "NODE_ENV=production webpack-dev-server --host 0.0.0.0 --mode production",
    "start:analyze": "NODE_ENV=development webpack-dev-server --inline --host 0.0.0.0 --hot --mode development --analyze",
    "build:dev": "yarn build:dist && NODE_ENV=development webpack --mode development --env.legacy=true",
    "build:staging": "yarn build:dist && NODE_ENV=staging webpack --mode production --env.legacy=true",
    "build": "yarn build:dist && NODE_ENV=production webpack --mode production --env.legacy=true",
    "build:analyze": "yarn build:dist && NODE_ENV=production webpack --mode production --analyze --profile --json > compilation-stats.json --env.legacy=true",
    "build-prerender:local": "yarn build:dev && yarn prerender && cp -rf ./prerendered/* ./dist",
    "lint": "NODE_ENV=production eslint --ext .js,.vue src",
    "lint:fix": "NODE_ENV=production eslint --ext .js,.vue src --fix",
    "build:dist": "rm -rf dist/* && cp staticdata/dist/** dist/"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-eslint": "8.0.1",
    "bezier-easing": "^2.1.0",
    "core-js": "^2",
    "fibers": "^4.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.2.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^0.23.0",
    "vue": "2.6.12",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "2.3.17",
    "vuetify-loader": "^1.3.0",
    "vuex": "^3.1.1",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.8.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "prerender-spa-plugin": "^3.4.0",
    "pug-lint-vue": "^0.1.3",
    "sync-request": "^6.1.0",
    "terser-webpack-plugin": "^2.3.5",
    "vue-pug-lint-loader": "^0.0.1",
    "vue-svg-loader": "^0.15.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not ie <= 8"
  ]
}
